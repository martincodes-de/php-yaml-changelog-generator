{
    "name": "martincodes-de/php-yaml-changelog-generator",
    "type": "library",
    "description": "Generate your changelog from yaml-files",
    "keywords": ["changelog"],
    "homepage": "https://github.com/martincodes-de/php-yaml-changelog-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Schneider",
            "email": "martin.schneider@techniknews.net",
            "homepage": "https://github.com/martincodes-de",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "ext-yaml": "*"
    },
    "autoload": {
        "psr-0": {
            "YamlChangelogGenerator": "src"
        }
    },
    
    "require-dev": {
        "phpstan/phpstan": "^1.8"
    }
}
